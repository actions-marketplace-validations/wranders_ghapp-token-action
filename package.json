    "clean": "npm run clean:dist clean:lib",
    "prebuild": "npm run clean && tsc",
    "build": "ncc build --minify"
  },
  "devDependencies": {
    "@actions/core": "^1.10.0",
    "@types/node": "^18.13.0",
